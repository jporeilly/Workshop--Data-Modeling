      <NamedSet name="Top 3 Territories">
        <Formula>
        <![CDATA[
        TopCount(
            [CUSTOMERS].[Customers].[Territory].Members,
            3,
            [Measures].[Sales]
        )
        ]]>
        </Formula>
    </NamedSet>
</Schema>
